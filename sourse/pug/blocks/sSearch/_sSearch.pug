			+e.item&attributes(attributes)(data-aos="fade-up")
				+e.row.row
					+e.col.col-md.order-2.order-md-1
						+e.content 
							if name
								+e.name!= name
							a.h3(href="#")!= title
							p!= text
							if date
								+e.date!= date
							+tags(...items)
					.col-md-auto.order-1.order-md-2
						+e.img-wrap
