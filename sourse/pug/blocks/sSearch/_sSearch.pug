mixin sSearchNothing(data)
	// start sSearch
	+b.SECTION.sSearch.section#sSearch&attributes(attributes)
		+e.wrap
			h1 Поиск
			+form-wrap
				+input('', 'text')
				button.btn
					+icon('search')
			+e.found--nothing
				+e.found-title Ничего не найдено.
				+e.found-text Пожалуйста, введите другой или скорректируйте текущий поисковой запрос.